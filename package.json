{
  "name": "@dz-web/finance-chart",
  "version": "0.0.4",
  "description": "finance chart for dz-web",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "dist": "npm run clean && webpack --config ./dev/webpack.config.js",
    "example": "rimraf example-dist/ && webpack --config ./dev/webpack.example.js",
    "start": "node ./dev/serve.js",
    "prepublishOnly": "npm run dist && npm run example",
    "pub": "npm publish --access public",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "ghpages": "npm run example && git subtree push --prefix example-dist/ origin gh-pages",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "yeluoqiuzhi",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "autoprefixer": "^8.2.0",
    "canvas": "^1.6.10",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^0.3.1",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {
    "@types/d3-array": "^1.2.1",
    "@types/d3-scale": "^2.0.0",
    "@types/d3-shape": "^1.2.2",
    "d3-array": "^1.2.1",
    "d3-scale": "^2.0.0",
    "d3-shape": "^1.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/dev/jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).{js,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
